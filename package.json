{
  "name": "finance",
  "version": "0.1.0",
  "author": "fe",
  "description": "Michelangelo-static-finance",
  "keywords": [
    "react",
    "router",
    "immutable",
    "webpack"
  ],
  "scripts": {
    "clean": "rimraf ./public/dist",
    "dll": "cross-env NODE_ENV=development webpack --progress --colors --config webpack.config.dll.babel.js && cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.dll.babel.js",
    "prebuild": "npm install --only=prod && npm run clean",
    "build": "cross-env NODE_ENV=production webpack --progress --colors --config webpack.config.js",
    "postbuild": "sh ./build.sh",
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --colors --hot --inline"
  },
